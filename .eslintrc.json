{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "plugin:react/jsx-runtime",
        "standard",
        "eslint-config-prettier"
    ],
    "settings": {
        "react":{
            "version": "detect"
        }
    },
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-hooks"
    ],
    "rules": {
        "react/display-name": "off",
        "react/react-in-jsx-scope": "off",
        "no-unused-vars":"warn"
    }
}
